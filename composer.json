{
    "name": "mr-vaco/nova-gallery",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova",
        "gallery"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ayvazyan10/nova-imagic": "^1.3",
        "mr-vaco/nova-panels": "^1.0.6",
        "mr-vaco/nova-statuses-manager": "^1.0",
        "whitecube/nova-flexible-content": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MrVaco\\NovaGallery\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrVaco\\NovaGallery\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
